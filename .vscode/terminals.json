{
  "autorun": true,
  "terminals": [
    {
      "name": "workspace",
      "cwd": "[cwd]",
      "execute": false,
      "command": "pnpm "
    },
    {
      "name": "workspace: watch",
      "cwd": "[cwd]",
      "execute": false,
      "command": "pnpm run w:dev:watch"
    },
    {
      "name": "renderer-pdf: create PDF",
      "cwd": "[cwd]/packages/renderer-pdf",
      "execute": false,
      "command": "pnpm run create:pdf"
    }
  ]
}
