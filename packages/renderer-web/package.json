{
  "name": "@document-generator/renderer-web",
  "version": "1.0.0",
  "private": true,
  "imports": {
    "#pkg/*": "./dist/*"
  },
  "scripts": {
    "pkg:nuke": "pnpm run pkg:nuke:artifacts && rimraf node_modules",
    "pkg:nuke:artifacts": "rimraf dist *.tsbuildinfo",
    "lint": "cross-env APPLY_HEAVY_RULES=true eslint --max-warnings 0",
    "pkg:lint": "pnpm run lint .",
    "pkg:lint:fix": "pnpm run pkg:lint --fix",
    "pkg:dev:watch": "next dev"
  },
  "dependencies": {
    "@document-generator/config": "workspace:1.0.0",
    "@pkerschbaum/ts-utils": "^0.0.1",
    "dayjs": "^1.11.7",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@next/env": "^13.1.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.11.0",
    "typescript": "^4.9.4"
  }
