{
  "name": "@document-generator/renderer-pdf",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "nuke": "pnpm run nuke:node-modules",
    "nuke:node-modules": "rimraf node_modules",
    "lint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix --max-warnings 0 .",
    "build-pdf": "ts-node --transpile-only ./src/index.ts",
    "build-pdf-watch": "nodemon --watch \"..\" --ext \"ts,tsx,css,json\" --exec \"pnpm run build-pdf\""
  },
  "dependencies": {
    "@document-generator/documents": "workspace:0.0.1",
    "@emotion/react": "^11.1.5",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "dayjs": "^1.10.4",
    "nodemon": "^2.0.7",
    "puppeteer": "^9.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
