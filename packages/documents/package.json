{
  "name": "@document-generator/documents",
  "private": true,
  "version": "1.0.0",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "postinstall": "ts-patch install -s",
    "nuke": "pnpm run nuke:build",
    "nuke:build": "rimraf dist *.tsbuildinfo",
    "build": "tsc -p ./tsconfig.project.json",
    "lint": "eslint --max-warnings 0",
    "lint:package": "pnpm run lint .",
    "lint:package:fix": "pnpm run lint:package --fix",
    "dev:watch": "tsc -p ./tsconfig.project.json --watch --preserveWatchOutput"
  },
  "dependencies": {
    "dayjs": "^1.10.4"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.3",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^17.0.2",
    "ts-patch": "^2.0.1",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.3",
    "@types/react": "^17.0.19",
    "react": "^17.0.2"
  }
}
